# Server configuration
PORT=3000
LOG_LEVEL=info
ALLOWED_ORIGINS=http://localhost:5173
# To allow multiple origins, separate them with commas:
# ALLOWED_ORIGINS=http://localhost:5173,https://your-frontend.example.com

# Azure AI Foundry credentials
AZURE_AI_FOUNDRY_ENDPOINT=https://<service>.services.ai.azure.com
AZURE_AI_FOUNDRY_PROJECT=<project>
AZURE_AI_FOUNDRY_AGENT_ID=<agent-id>
AZURE_TENANT_ID=<tenant-id>
AZURE_CLIENT_ID=<client-id>
AZURE_CLIENT_SECRET=<client-secret>
